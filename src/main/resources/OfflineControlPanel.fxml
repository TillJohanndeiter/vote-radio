<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="playerPane" prefHeight="185.0" prefWidth="157.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.techfak.gse.tjohanndeiter.controller.JukeBoxController">
    <Button fx:id="start" layoutX="58.0" layoutY="44.0" mnemonicParsing="false" onAction="#startPlay" text="Start"/>
    <Button fx:id="stop" layoutX="58.0" layoutY="115.0" mnemonicParsing="false" onAction="#stopPlay" text="Stop"/>
    <Button fx:id="pauseOrResumeButton" layoutX="55.0" layoutY="80.0" mnemonicParsing="false" onAction="#pauseOrResume"
            text="Pause"/>
</AnchorPane>
